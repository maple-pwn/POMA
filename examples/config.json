{
  "name": "baseline_evaluation",
  "description": "Baseline evaluation of multiple LLMs on Pwn challenges",
  "models": [
    {
      "provider": "openai",
      "model_name": "gpt-4o",
      "api_key_env": "OPENAI_API_KEY",
      "temperature": 0.0,
      "max_tokens": 4096,
      "timeout": 120
    },
    {
      "provider": "anthropic",
      "model_name": "claude-3-5-sonnet-20241022",
      "api_key_env": "ANTHROPIC_API_KEY",
      "temperature": 0.0,
      "max_tokens": 4096,
      "timeout": 120
    },
    {
      "provider": "deepseek",
      "model_name": "deepseek-chat",
      "api_key_env": "DEEPSEEK_API_KEY",
      "temperature": 0.0,
      "max_tokens": 4096,
      "timeout": 120
    }
  ],
  "challenge_ids": [],
  "ablation_conditions": [
    "full_pipeline",
    "gt_phase0",
    "gt_phase0_1",
    "gt_phase0_1_2"
  ],
  "max_iterations": 10,
  "parallel_workers": 1,
  "output_dir": "results"
}
